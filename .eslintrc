{
	"rules": {
		"indent": [
			2,
			"tab",
			{"SwitchCase": 1}
		],
		"quotes": [
			2,
			"single",
			{"allowTemplateLiterals": true}
		],
		"semi": [
			2,
			"always"
		],
		"comma-dangle": [
			0,
			"always-multiline"
		],
		"no-console": [
			1,
		],
		"no-unused-vars": [
			1,
		]
	},
	"env": {
		"node": true,
		"es6": true
	},
	"extends": "eslint:recommended"
}
